{
    "version": 2,
    "builds": [
        {
            "src": "simple-client/**",
            "use": "@vercel/static"
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "/simple-client/$1"
        }
    ],
    "headers": [
        {
            "source": "/(.*).js",
            "headers": [
                {
                    "key": "Content-Type",
                    "value": "application/javascript; charset=utf-8"
                }
            ]
        }
    ]
}